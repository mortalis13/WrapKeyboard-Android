<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
  android:keyWidth="25%p"
  android:keyHeight="@dimen/key_height"
  android:horizontalGap="0px"
  android:verticalGap="0px" 
>
  <Row>
    <Key android:codes="60001" android:keyLabel="Home" android:keyEdgeFlags="left"/>
    <Key android:codes="60002" android:keyIcon="@drawable/sym_keyboard_up" android:isRepeatable="true" />
    <Key android:codes="60003" android:keyLabel="End" />
    <Key android:keyLabel="/" android:keyWidth="12.5%p"/>
    <Key android:keyLabel="\\" android:keyWidth="12.5%p" android:keyEdgeFlags="right"/>
  </Row> 
  
  <Row>
    <Key android:codes="60004" android:keyIcon="@drawable/sym_keyboard_left" android:isRepeatable="true" android:keyEdgeFlags="left"/>
    <Key android:codes="60011" android:keyLabel="Select" android:isSticky="true"/>
    <Key android:codes="60006" android:keyIcon="@drawable/sym_keyboard_right" android:isRepeatable="true" />
    <Key android:keyLabel="[" android:keyWidth="12.5%p"/>
    <Key android:keyLabel="]" android:keyWidth="12.5%p" android:keyEdgeFlags="right"/>
  </Row> 
  
  <Row>
    <Key android:codes="60007" android:keyLabel="All" android:keyEdgeFlags="left"/>
    <Key android:codes="60005" android:keyIcon="@drawable/sym_keyboard_down" android:isRepeatable="true" />
    <Key android:codes="60012" android:keyLabel="Clear"/>
    <Key android:keyLabel=":" android:keyWidth="12.5%p"/>
    <Key android:keyLabel=";" android:keyWidth="12.5%p" android:keyEdgeFlags="right"/>
  </Row> 
  
  <Row>
    <Key android:codes="60008" android:keyLabel="Copy" android:keyEdgeFlags="left"/>
    <Key android:codes="60009" android:keyLabel="Cut"/>
    <Key android:codes="60010" android:keyLabel="Paste"/>
    <Key android:keyLabel="&amp;" android:keyWidth="12.5%p"/>
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="12.5%p" android:keyEdgeFlags="right" android:isRepeatable="true" android:isModifier="true" />
  </Row> 
  
  <Row android:keyWidth="12.5%p">
    <Key android:codes="-2" android:keyLabel="ABC" android:isModifier="true" android:keyEdgeFlags="left"/>
    <Key android:keyLabel="\@" android:isModifier="true"/>
    <Key android:keyLabel="_" android:isModifier="true"/>
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:isRepeatable="true" android:keyWidth="25%p" android:isModifier="true"/>
    <Key android:keyLabel="," android:isModifier="true"/>
    <Key android:keyLabel="." android:isModifier="true"/>
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyEdgeFlags="right" android:isModifier="true"/>
  </Row> 
</Keyboard>
