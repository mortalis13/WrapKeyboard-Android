<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
  android:keyWidth="12.5%p"
  android:keyHeight="@dimen/key_height"
  android:horizontalGap="0px"
  android:verticalGap="0px"
>
  <Row>
    <Key android:keyLabel="a" android:popupKeyboard="@xml/keyboard_popup_template"
      android:popupCharacters="@string/alternates_for_a" android:keyEdgeFlags="left"/>
    <Key android:keyLabel="b"/>
    <Key android:keyLabel="c"/>
    <Key android:keyLabel="d"/>
    <Key android:keyLabel="e"/>
    <Key android:keyLabel="f"/>
    <Key android:keyLabel="g"/>
    <Key android:keyLabel="h" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:keyLabel="i" android:keyEdgeFlags="left"/>
    <Key android:keyLabel="j"/>
    <Key android:keyLabel="k"/>
    <Key android:keyLabel="l"/>
    <Key android:keyLabel="m"/>
    <Key android:keyLabel="n"/>
    <Key android:keyLabel="o"/>
    <Key android:keyLabel="p" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:keyLabel="q" android:keyEdgeFlags="left"/>
    <Key android:keyLabel="r"/>
    <Key android:keyLabel="s"/>
    <Key android:keyLabel="t"/>
    <Key android:keyLabel="u"/>
    <Key android:keyLabel="v"/>
    <Key android:keyLabel="w"/>
    <Key android:keyLabel="x" android:keyEdgeFlags="right" />
  </Row>

  <Row>
    <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:keyEdgeFlags="left" android:isModifier="true"/>
    <Key android:keyLabel="y"/>
    <Key android:keyLabel="z"/>
    <Key android:keyLabel="&amp;"/>
    <Key android:keyLabel="-"/>
    <Key android:keyLabel="?"/>
    <Key android:keyLabel="/"/>
    <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyEdgeFlags="right" android:isModifier="true" android:isRepeatable="true"/>
  </Row>

  <Row>
    <Key android:codes="-2" android:keyLabel="123" android:isModifier="true" android:keyEdgeFlags="left"/>
    <Key android:codes="55000" android:keyLabel="EN" android:isModifier="true"/>
    <Key android:keyLabel="_" android:isModifier="true"/>
    <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:keyWidth="25%p" android:isRepeatable="true" android:isModifier="true"/>
    <Key android:keyLabel="," android:isModifier="true"/>
    <Key android:keyLabel="." android:isModifier="true"/>
    <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyEdgeFlags="right" android:isModifier="true"/>
  </Row>

</Keyboard>
