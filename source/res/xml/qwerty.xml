<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="13.4%p"
    android:horizontalGap="1%p"
    android:verticalGap="0.5%p" 
    android:keyHeight="@dimen/key_height" 
    >

    <Row>
        <Key android:codes="97"  android:keyLabel="a" android:horizontalGap="0px" android:keyEdgeFlags="left"/>
        <Key android:codes="98"  android:keyLabel="b"/> 
        <Key android:codes="99"  android:keyLabel="c"/>
        <Key android:codes="100" android:keyLabel="d"/>
        <Key android:codes="101" android:keyLabel="e"/>
        <Key android:codes="102" android:keyLabel="f"/>
        <Key android:codes="103" android:keyLabel="g" android:keyWidth="13.6%p" android:keyEdgeFlags="right"/>
    </Row>
        
    
    <Row>
        <Key android:codes="104" android:keyLabel="h" android:horizontalGap="0px" android:keyEdgeFlags="left"/>
        <Key android:codes="105" android:keyLabel="i"/>
        <Key android:codes="106" android:keyLabel="j"/>
        <Key android:codes="107" android:keyLabel="k"/>
        <Key android:codes="108" android:keyLabel="l"/>
        <Key android:codes="109" android:keyLabel="m"/>
        <Key android:codes="110" android:keyLabel="n" android:keyWidth="13.6%p" android:keyEdgeFlags="right"/>
    </Row>    
        
    
    <Row>
        <Key android:codes="111" android:keyLabel="o" android:horizontalGap="0px" android:keyEdgeFlags="left"/>
        <Key android:codes="112" android:keyLabel="p"/>
        <Key android:codes="113" android:keyLabel="q"/>
        <Key android:codes="114" android:keyLabel="r"/>
        <Key android:codes="115" android:keyLabel="s"/>
        <Key android:codes="116" android:keyLabel="t"/>
        <Key android:codes="117" android:keyLabel="u" android:keyWidth="13.6%p" android:keyEdgeFlags="right"/>
    </Row>
        
    
    <Row> 
        <Key android:codes="-1" android:keyIcon="@drawable/sym_keyboard_shift" android:isModifier="true"
             android:horizontalGap="0px" android:keyEdgeFlags="left"/>
        <Key android:codes="118" android:keyLabel="v"/>
        <Key android:codes="119" android:keyLabel="w"/>
        <Key android:codes="120" android:keyLabel="x"/>
        <Key android:codes="121" android:keyLabel="y"/>
        <Key android:codes="122" android:keyLabel="z"/>
        <Key android:codes="-5" android:keyIcon="@drawable/sym_keyboard_delete" android:keyWidth="13.6%p" 
             android:keyEdgeFlags="right" android:isModifier="true" android:isRepeatable="true"/>
    </Row>
    
    <!-- <Row android:rowEdgeFlags="bottom"> -->
    
    <Row>
      <Key android:codes="-2" android:horizontalGap="0px" android:keyLabel="123" android:isModifier="true" android:keyEdgeFlags="left"/>
      <Key android:codes="55000" android:keyLabel="ES" android:isModifier="true"/>
      <Key android:codes="32" android:keyIcon="@drawable/sym_keyboard_space" android:isRepeatable="true" android:keyWidth="32.5%p" android:isModifier="true"/>
      <Key android:codes="46" android:keyLabel="." android:keyWidth="11%p" android:isModifier="true"/>
      <Key android:codes="47" android:keyLabel="/" android:keyWidth="11%p" android:isModifier="true"/>
      <Key android:codes="10" android:keyIcon="@drawable/sym_keyboard_return" android:keyEdgeFlags="right" android:isModifier="true"/>
  	</Row>
  
</Keyboard>
    